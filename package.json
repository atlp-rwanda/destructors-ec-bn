{
  "name": "destructors-ec-bn",
  "version": "1.0.0",
  "description": "Destructors ecommerce team project",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --exec npx babel-node src/index",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
    "migrate:down": "sequelize db:migrate:undo:all",
    "run-tests": "cross-env NODE_ENV=test jest --coverage --verbose --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "run-after-tests": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "test": "npm-run-all run-tests run-after-tests --continue-on-error",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/destructors-ec-bn.git"
  },
  "author": "Destructors Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/destructors-ec-bn/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/destructors-ec-bn#readme",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "cloudinary": "^1.35.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cryptr": "^6.1.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jest": "^29.5.0",
    "joi": "^17.8.3",
    "joi-password-complexity": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.1",
    "sequelize-cli": "^6.6.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "stripe": "^11.18.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0",
    "uuid-validate": "^0.0.3",
    "winston": "^3.9.0",
    "winston-sequelize": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5"
  }
}
