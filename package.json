{
  "name": "destructors-ec-bn",
  "version": "1.0.0",
  "description": "Destructors ecommerce team project",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --exec npx babel-node src/index",
    "migrate": "sequelize db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrate:down": "sequelize db:migrate:undo:all",
    "test": "cross-env NODE_ENV=test jest --coverage --testTimeout=20000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/destructors-ec-bn.git"
  },
  "author": "Destructors Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/destructors-ec-bn/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/destructors-ec-bn#readme",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "cryptr": "^6.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jest": "^29.5.0",
    "joi": "^17.8.3",
    "joi-password-complexity": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.1",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "coveralls": "^3.1.1",
    "nodemon": "^2.0.21"
  }
}
